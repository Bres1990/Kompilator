{ sito Eratostenesa }
VAR
    n j sito[100]
BEGIN
    n := 100-1;
    FOR i FROM n DOWNTO 2 DO
        sito[i] := 1;
    ENDFOR
    j := 1;
    FOR i FROM j TO n DO
        sito[i] := j
        IF sito[i] <> 0 THEN
            j := i + i;
            WHILE j <= n DO
                sito[j] := 0;
                j := j + i;
            ENDWHILE
            WRITE i;
        ELSE
          SKIP;
        ENDIF
    ENDFOR
END


